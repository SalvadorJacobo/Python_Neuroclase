tama単o = int(input('Definir tama単o de array: '))
variableNombres = []
variableLongitud = []

for nombres in range(0, tama単o):
    variableNombres.append(input('Dame el nombre: '))
print(variableNombres)

for longitud in range(0, tama単o):
    variableLongitud.append(len(variableNombres[longitud]))
print(variableLongitud)
